{
  "locals": {
    "url": "http://localhost:8080",
    "name": "Nook Harquail",
    "owner": "Nook Harquail",
    "description": "Ramblings of an immor(t)al demigod"
  },
  "plugins": [
    "./plugins/paginator.coffee",
    "wintersmith-tag",
    "wintersmith-autoprefixer-sass"
  ],
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "jade": {
    "pretty": true
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "paginator": {
    "articles": "portfolio",
    "perPage": 12,
    "first": "/portfolio/index.html",
    "filename": "portfolio/%d/index.html"
  },
  "tag": {
    "filename": "/portfolio/%s/%d/index.html"
  },
  "ignore": [
    "**/*.psb",
    "**/*.psd",
    "**/*.ai",
    "**/*indd",
    "**/*.conf",
    "**/.DS_Store"
  ],
  "output": "harquail.com"
}
